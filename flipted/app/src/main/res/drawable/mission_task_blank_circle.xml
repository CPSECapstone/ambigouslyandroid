<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    >
<item>
    <aapt:attr name="android:drawable">
    <shape
        android:shape="oval"
        >
        <size
            android:width="64dp"
            android:height="64dp"
            />
        <stroke
            android:width="4dp"
            android:color="@color/blue1"/>
        <solid
            android:color="@color/white"/>
    </shape>
    </aapt:attr>
</item>

</layer-list>